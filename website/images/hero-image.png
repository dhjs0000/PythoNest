<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 500">
  <!-- 背景 -->
  <rect width="800" height="500" rx="8" fill="#f5f5f5" stroke="#dddddd" stroke-width="2"/>
  
  <!-- 窗口标题栏 -->
  <rect x="0" y="0" width="800" height="40" rx="8" fill="#4a86e8"/>
  <circle cx="25" cy="20" r="8" fill="#ff6058"/>
  <circle cx="50" cy="20" r="8" fill="#ffbd2e"/>
  <circle cx="75" cy="20" r="8" fill="#28ca42"/>
  <text x="400" y="25" font-family="Arial" font-size="14" fill="white" text-anchor="middle">PythoNest</text>

  <!-- 侧边栏 -->
  <rect x="0" y="40" width="200" height="460" fill="#ffffff" stroke="#dddddd" stroke-width="1"/>
  
  <!-- 侧边栏菜单项 -->
  <rect x="10" y="60" width="180" height="40" rx="4" fill="#e6f0ff"/>
  <text x="30" y="85" font-family="Arial" font-size="14" fill="#4a86e8">Python 版本</text>
  
  <rect x="10" y="110" width="180" height="40" rx="4" fill="#ffffff"/>
  <text x="30" y="135" font-family="Arial" font-size="14" fill="#666666">虚拟环境</text>
  
  <rect x="10" y="160" width="180" height="40" rx="4" fill="#ffffff"/>
  <text x="30" y="185" font-family="Arial" font-size="14" fill="#666666">包管理</text>

  <!-- 主内容区 -->
  <rect x="220" y="60" width="560" height="420" rx="4" fill="#ffffff" stroke="#dddddd" stroke-width="1"/>
  
  <!-- 标题 -->
  <text x="250" y="100" font-family="Arial" font-size="24" font-weight="bold" fill="#333333">Python 版本管理</text>
  
  <!-- 版本列表 -->
  <rect x="250" y="140" width="500" height="50" rx="4" fill="#f9f9f9" stroke="#dddddd" stroke-width="1"/>
  <text x="270" y="170" font-family="Arial" font-size="14" fill="#333333">Python 3.10.0</text>
  <rect x="430" y="150" width="80" height="30" rx="4" fill="#4a86e8"/>
  <text x="470" y="170" font-family="Arial" font-size="12" fill="white" text-anchor="middle">活跃</text>
  <rect x="650" y="150" width="80" height="30" rx="4" fill="#ffffff" stroke="#4a86e8" stroke-width="1"/>
  <text x="690" y="170" font-family="Arial" font-size="12" fill="#4a86e8" text-anchor="middle">设为默认</text>
  
  <rect x="250" y="200" width="500" height="50" rx="4" fill="#f9f9f9" stroke="#dddddd" stroke-width="1"/>
  <text x="270" y="230" font-family="Arial" font-size="14" fill="#333333">Python 3.9.7</text>
  <rect x="430" y="210" width="80" height="30" rx="4" fill="#dddddd"/>
  <text x="470" y="230" font-family="Arial" font-size="12" fill="#666666" text-anchor="middle">非活跃</text>
  <rect x="650" y="210" width="80" height="30" rx="4" fill="#ffffff" stroke="#4a86e8" stroke-width="1"/>
  <text x="690" y="230" font-family="Arial" font-size="12" fill="#4a86e8" text-anchor="middle">设为默认</text>
  
  <rect x="250" y="260" width="500" height="50" rx="4" fill="#f9f9f9" stroke="#dddddd" stroke-width="1"/>
  <text x="270" y="290" font-family="Arial" font-size="14" fill="#333333">Python 3.8.12</text>
  <rect x="430" y="270" width="80" height="30" rx="4" fill="#dddddd"/>
  <text x="470" y="290" font-family="Arial" font-size="12" fill="#666666" text-anchor="middle">非活跃</text>
  <rect x="650" y="270" width="80" height="30" rx="4" fill="#ffffff" stroke="#4a86e8" stroke-width="1"/>
  <text x="690" y="290" font-family="Arial" font-size="12" fill="#4a86e8" text-anchor="middle">设为默认</text>
  
  <!-- 按钮 -->
  <rect x="250" y="340" width="160" height="40" rx="4" fill="#4a86e8"/>
  <text x="330" y="365" font-family="Arial" font-size="14" fill="white" text-anchor="middle">安装新版本</text>
  
  <rect x="430" y="340" width="160" height="40" rx="4" fill="#ffffff" stroke="#ff6058" stroke-width="1"/>
  <text x="510" y="365" font-family="Arial" font-size="14" fill="#ff6058" text-anchor="middle">卸载选中版本</text>
</svg> 